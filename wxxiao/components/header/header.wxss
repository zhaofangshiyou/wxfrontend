/* components/header/header.wxss */
.header{
    width: 100%;
    height: 128rpx;
    background: -webkit-linear-gradient(left,#F96432, #FD9246); /* Safari 5.1 - 6.0 */
    background: -o-linear-gradient(right,#F96432, #FD9246); /* Opera 11.1 - 12.0 */
    background: -moz-linear-gradient(right,#F96432ed, #FD9246); /* Firefox 3.6 - 15 */
    background: linear-gradient(to right,#F96432, #FD9246); /* 标准的语法 */ 
    box-sizing: border-box;
    padding: 68rpx 30rpx 26rpx 30rpx;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 9999;
}
.header_title{
    height: 34rpx;
    line-height: 34rpx;
    font-size:34rpx;
    color:rgba(255,255,255,1);
    text-align: center;
    width: 100%;
}
.goBack{
    position: absolute;
    top: 50rpx;
    left: 10rpx;
    padding: 20rpx;
}
.left_icon{
    width: 15rpx;
    height: 30rpx;
}