/* pages/succeCard/scanSuccess.wxss */
.erweima{
  position: fixed;
  top:0;
  background: #f5f5f5;
  width: 100%;
  height: 100%;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
}

.code_box{
  width: 100%;
  height: 870rpx;
  background: #FFF;
  text-align: center;
  padding-top: 178rpx;
  font-size: 0;
}
.success_icon{
  width: 164rpx;
  height: 164rpx;
}
.title{
  height:30rpx;
  font-size:30rpx;
  color:rgba(51,51,51,1);
  line-height:30rpx;
  margin-top: 58rpx;
  margin-bottom: 30rpx;
}
.note{
  height:30rpx;
  font-size:30rpx;
  color:rgba(153,153,153,1);
  line-height:30rpx;
  margin-bottom: 130rpx;
}
.goBack{
  width:570rpx;
  height:90rpx;
  background:linear-gradient(135deg,rgba(249,100,50,1),rgba(253,146,70,1));
  box-shadow:0px 4rpx 10rpx 0px rgba(249,100,50,0.3);
  border-radius:45rpx;
  color: #fff;
  text-align: center;
  line-height: 90rpx;
  font-size: 30rpx;
  margin: 0 auto;
}